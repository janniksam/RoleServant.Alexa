{
  "Logging": {
    "File": {
      "BasePath": "data/Logs",
      "EnsureBasePath": true,
      "FileNameMappings": {
        "Default": "roleshuffle.log"
      },
      "FileEncoding": "utf-8",
      "DateFormat": "yyyyMMdd",
      "CounterFormat": "000",
      "MaxFileSize": 10485760,
      "LogLevel": {
        "Default": "Information",
        "System": "Warning",
        "Microsoft": "Warning",
      },
      "IncludeScopes": false,
      "MaxQueueSize": 100
    }
  },
  // global filter settings
  "LogLevel": {
    "Default": "Information"
  }
}
